<%- model_class = User -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<div id="list_of_user">
   <%= render partial: "list_of_users", locals: { users: User.all } %>
</div>
<div id="user_form">
   <%= render partial: "form", locals: { user: User.new } %>
</div>

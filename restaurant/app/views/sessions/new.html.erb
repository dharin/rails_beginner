<h1> Login </h1>
<%= alert %>
<%= form_tag  session_path do %>
<div class="field">
<%= label_tag :email %><br />
<%= text_field_tag :username %>
</div>
<div class="field">
<%= label_tag :password %><br />
<%= password_field_tag :password %>
</div>
<div class="actions">
<%= submit_tag 'Login' %>
</div>

<% end %>